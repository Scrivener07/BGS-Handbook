{
  "liquid.engine": "jekyll",
  "liquid.config.baseDir": "./docs",
  "[liquid]": {
    "editor.defaultFormatter": "sissel.shopify-liquid",
    "editor.formatOnSave": true
  },
  "files.associations": {
    "*.html": "liquid",
    "*.js": "liquid-javascript",
    "*.css": "liquid-css",
    "*.scss": "liquid-scss"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/desktop.ini": true,
    "**/$RECYCLE.BIN": true,
    "**/System Volume Information": true
  },
  "editor.tabSize": 2,
  "cSpell.ignoreWords": [
    "AACT",
    "ACTI",
    "activatemenu",
    "alegendv",
    "amet",
